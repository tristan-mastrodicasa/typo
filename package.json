    "url": "https://github.com/tristan-mastrodicasa/typo/issues"
  },
  "homepage": "https://github.com/tristan-mastrodicasa/typo#readme",
  "devDependencies": {
    "node-sass": "^4.13.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.7.4"
  }
