{
  "name": "typo",
  "version": "1.0.0",
  "description": "Auto generate typography for your website or web app",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-js && npm run build-scss",
    "clean": "rm public/css/style.css public/js/main.js",
    "build-js": "npx tsc -w src/app/index.ts --out public/js/main.js",
    "build-scss": "npx node-sass src/sass/main.scss public/css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tristan-mastrodicasa/typo.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tristan-mastrodicasa/typo/issues"
  },
  "homepage": "https://github.com/tristan-mastrodicasa/typo#readme",
  "devDependencies": {
    "node-sass": "^4.13.0",
    "typescript": "^3.7.4"
  }
}
